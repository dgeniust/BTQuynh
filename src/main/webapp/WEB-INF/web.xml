<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>Web login</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>jsp</servlet-name>
    <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
    <init-param>
      <param-name>mappedfile</param-name>
      <param-value>false</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <display-name>RegisterController</display-name>
    <servlet-name>RegisterController</servlet-name>
    <servlet-class>vn.iostar.controller.RegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterController</servlet-name>
    <url-pattern>/RegisterController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserController</display-name>
    <servlet-name>UserController</servlet-name>
    <servlet-class>vn.iostar.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ManagerController</display-name>
    <servlet-name>ManagerController</servlet-name>
    <servlet-class>vn.iostar.controller.ManagerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerController</servlet-name>
    <url-pattern>/ManagerController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutController</display-name>
    <servlet-name>LogoutController</servlet-name>
    <servlet-class>vn.iostar.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutController</servlet-name>
    <url-pattern>/LogoutController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FPController</display-name>
    <servlet-name>FPController</servlet-name>
    <servlet-class>vn.iostar.controller.FPController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FPController</servlet-name>
    <url-pattern>/FPController</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>sitemesh</filter-name>
    <filter-class>
			org.sitemesh.config.ConfigurableSiteMeshFilter
		</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>sitemesh</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>HomePageController</display-name>
    <servlet-name>HomePageController</servlet-name>
    <servlet-class>vn.iostar.controller.HomePageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomePageController</servlet-name>
    <url-pattern>/HomePageController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CategoryController</display-name>
    <servlet-name>CategoryController</servlet-name>
    <servlet-class>controller.CategoryController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoryController</servlet-name>
    <url-pattern>/CategoryController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>VideoController</display-name>
    <servlet-name>VideoController</servlet-name>
    <servlet-class>controller.VideoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VideoController</servlet-name>
    <url-pattern>/VideoController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UsersController</display-name>
    <servlet-name>UsersController</servlet-name>
    <servlet-class>controller.UsersController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsersController</servlet-name>
    <url-pattern>/UsersController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DownloadImageController</display-name>
    <servlet-name>DownloadImageController</servlet-name>
    <servlet-class>controller.DownloadImageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownloadImageController</servlet-name>
    <url-pattern>/DownloadImageController</url-pattern>
  </servlet-mapping>
</web-app>